<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet"> 
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>  

  
    

    

    <main class="">

      {{ content | safe }}
    </main>

    <footer class="main-footer py-20">
      <div class="container">
        <div class="main-footer-content mx-auto flex items-center ">
          <div class="team-message-video" >
            <img style="" src="/img/team-message-video.png" alt="">
          </div>
          <nav class="footer-navigation ml-12">
            <div>       
              <img style="max-width: 190px;" src="/img/logo-black.svg" alt="">
              {#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}
              <ul class="mt-4">
              {%- for entry in collections.all | eleventyNavigation %}
                <li class="text-2xl mb-4"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
              {%- endfor %}
              </ul> 
              <a href="" class="text-lg font-bold underline mt-8 inline-block">En savoir plus sur creative circle premium</a>
              <p class="text-sm mt-8 inline-block">Copyright Â© Creative Circle</a>

            </div>
          </nav>
          
        </div>
        
      </div>
    </footer>
    </footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>

<style>
  body {
    min-height: 100vh;
  }
  .container {
    max-width: 1200px;
    margin: auto;
  }

</style>

</html>
